{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Run JavaFX App",
            "request": "launch",
            "mainClass": "AppLauncher",
            "vmArgs": "-Dprism.order=sw -Dprism.verbose=true --module-path C:/JavaFX/javafx-sdk-25.0.1/lib --add-modules javafx.controls,javafx.fxml",
            "classPaths": [
                "${workspaceFolder}/bin",
                "${workspaceFolder}/lib/*"
            ]
        }
    ]
}